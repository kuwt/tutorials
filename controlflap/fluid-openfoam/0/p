FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.3;
    }

    outlet
    {
        type            zeroGradient;
    }

    flap
    {
        type            zeroGradient;
    }
    
    upperWall
    {
        type            zeroGradient;
    }

    lowerWall
    {
        type            zeroGradient;
    }

    rightWall
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

